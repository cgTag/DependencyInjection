{
    "name": "cgtag/dependency-injection",
    "description": "The PHP 7 dependency injection container for cgTag",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cgTag",
            "homepage": "https://cgTag.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/cgTag/DependencyInjection/issues",
        "source": "https://github.com/cgTag/DependencyInjection"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "cgTag\\DI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cgTag\\DI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage"
    }
}
