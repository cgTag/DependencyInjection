{
    "name": "cgtag/php-di",
    "description": "The PHP 7 dependency injection container for cgTag",
    "keywords": ["di","dependency injection","library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cgTag",
            "homepage": "https://cgTag.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/cgTag/php-di/issues",
        "source": "https://github.com/cgTag/php-di"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7,<6"
    },
    "autoload": {
        "psr-4": {
            "cgTag\\DI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cgTag\\DI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "coverage": "phpunit"
    }
}
